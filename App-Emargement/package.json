{
  "name": "app-emargement",
  "version": "1.0.0",
  "description": "Application de génération de feuilles d'émargement",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:frontend": "npx vite --config vite.config.mjs",
    "build": "npx vite build --config vite.config.mjs",
    "preview": "npx vite preview --config vite.config.mjs",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "dev:codespaces": "concurrently \"npm run dev\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ejs": "^3.1.10",
    "morgan": "^1.10.0",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "puppeteer": "^22.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "vite": "^7.1.0"
  },
  "keywords": [
    "émargement",
    "pdf",
    "api",
    "react"
  ],
  "author": "Hugo HOUNTONDJI - Chef Projet Qualité",
  "license": "MIT"
}
