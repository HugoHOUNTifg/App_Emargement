{
  "version": 2,
  "builds": [
    {
      "src": "App Emargement/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "App Emargement/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "App Emargement/dist/$1"
    }
  ],
  "buildCommand": "cd \"App Emargement\" && npm install && npx vite build --config vite.config.mjs",
  "outputDirectory": "App Emargement/dist"
}
