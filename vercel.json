{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/[...all].js", "use": "@vercel/node", "config": { "maxDuration": 60, "memory": 1536 } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[...all].js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
